<h2>Posts New</h2>



    <%= render "layouts/flash" %>

    <%= bootstrap_form_for @post do |f| %>
      <%= f.text_field :title, placeholder: "Enter your title", icon: "pencil" %>
      <%= f.text_field :link, placeholder: "Enter your link", icon: "link" %>
      <%= f.submit "Create Post", class: "btn btn-primary" %>
    <% end %>

